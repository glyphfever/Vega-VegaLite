{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Slope graph showing the change in yeild for barley sites",
    "data": {
        "url":"https://raw.githubusercontent.com/vega/vega-datasets/next/data/barley.json"
    },
    "mark":"line",
    "encoding": {
        "x":{
            "field":"date",
            "type": "temporal",
            "axis":{"grid": false}
        },
        "y":{
            "field":"price",
            "type": "quantitative",
            "axis":{"title":"Price"}
        },
 
        "color":{
            "field":"symbol",
            "type": "nominal"

        }
    }
 

}